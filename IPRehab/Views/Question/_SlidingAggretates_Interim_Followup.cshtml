                <div class="summary header" title="Performance">Perfor-mance</div>
            </div>
            <div class="flex-start-row-nowrap">
                <div class="summary header">Self-Care</div>
                <div class="summary header" title="self-care score" id="self_care_aggregate_score">0</div>
            </div>
            <div class="flex-start-row-nowrap">
                <div class="summary header">Mobi-lity</div>
                <div class="summary header" title="mobility score" id="mobility_aggregate_score">0</div>
            </div>
            <div class="flex-start-row-nowrap">
                <div class="summary header">Total</div>
                <div class="summary header" title="grand total" id="grand_total">0</div>
            </div>
        </div>
    </div>
</div>

