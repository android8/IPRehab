{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "module": "ES6",
    "target": "ES6",
    "paths": {
      "appScripts": [ "./Scripts/*" ],
      "nodeModules": [ "./node_modules/*" ]
    },
    "skipLibCheck": true
  },
  "compileOnSave": true
}
